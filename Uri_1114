import java.util.Scanner;

public class Uri_1114 {
    public static void main(String[] args) {
        Scanner tecla = new Scanner(System.in);
        int senha;
        while (true){
            senha = tecla.nextInt();
            if (senha == 2002){
                System.out.println("Acesso Permitido");
                break;
            }else {
                System.out.println("Senha Invalida");
            }
        }
    }
}
